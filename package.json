{
  "name": "auto-open-api",
  "version": "1.0.0",
  "description": "auto-open-api",
  "main": "build/index.js",
  "watch": {
    "compile-ts": {
      "patterns": [
        "src"
      ],
      "extensions": "ts"
    },
    "copy-static-files": {
      "patterns": [
        "src"
      ],
      "extensions": [
        "json",
        "html",
        "js",
        "png"
      ]
    }
  },
  "scripts": {
    "dev": "npm-watch",
    "copy-static-files": "rsync -zarv $(find ./src -print | grep 'json\\|html\\|js\\|png') ./dist",
    "lint": "prettier --ignore-path .gitignore . -w",
    "compile": "tsc --sourceMap",
    "clean": "rm -rf dist",
    "test": "jest"
  },
  "author": "Leonardo Vogel Dalcin",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/leonardodalcin/chrome-open-api-generetor.git"
  },
  "dependencies": {
    "generate-schema": "^2.6.0",
    "openapi-types": "^7.0.0"
  },
  "devDependencies": {
    "@types/chrome": "0.0.125",
    "@types/jest": "^26.0.15",
    "jest": "^26.6.1",
    "npm-watch": "^0.7.0",
    "prettier": "^2.2.0",
    "ts-jest": "^26.4.2",
    "ts-loader": "^8.0.7",
    "typescript": "~4.0.3"
  }
}
